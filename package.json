{
  "name": "edmunds",
  "version": "0.1.0",
  "description": "Edmunds is a Express based framework for building web applications with a firm structure to start from.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run tsc:watch",
    "tsc": "rm -rf dist && tsc",
    "tsc:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --exclude '**/*.d.ts' --exclude 'node_modules/**/*.ts' '**/*.ts'",
    "lint:watch": "nodemon --ext ts --exec npm run lint",
    "test": "NODE_ENV=testing mocha --opts mocha.opts '**/*.spec.ts'",
    "test:cover": "NODE_ENV=testing nyc mocha --opts mocha.opts '**/*.spec.ts'",
    "test:watch": "NODE_ENV=testing mocha --watch --opts mocha.opts '**/*.spec.ts'",
    "docs": "docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LowieHuyghe/edmunds-js.git"
  },
  "keywords": [
    "express",
    "framework",
    "javascript"
  ],
  "author": "Lowie Huyghe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LowieHuyghe/edmunds-js/issues"
  },
  "homepage": "https://github.com/LowieHuyghe/edmunds-js#README",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "commander": "^2.14.1",
    "config": "^1.29.4",
    "express": "^4.16.2",
    "inquirer": "^5.1.0",
    "joi": "^13.1.2",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/config": "0.0.34",
    "@types/express": "^4.11.1",
    "@types/inquirer": "0.0.41",
    "@types/joi": "^13.0.5",
    "@types/memcached": "^2.2.5",
    "@types/mocha": "^5.1.0",
    "@types/node": "^10.0.3",
    "@types/redis": "^2.8.5",
    "@types/sinon": "^4.3.3",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codecov": "^3.0.0",
    "docsify-cli": "^4.2.0",
    "firebase-admin": "^5.8.2",
    "firebase-functions": "^1.0.2",
    "import-fresh": "^2.0.0",
    "memcached": "^2.2.2",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "nyc": "^11.4.1",
    "redis": "^2.8.0",
    "sinon": "^5.0.7",
    "source-map-support": "^0.5.3",
    "sql.js": "^0.5.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2"
  }
}

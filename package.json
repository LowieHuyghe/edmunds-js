{
  "name": "edmunds",
  "version": "0.0.2",
  "description": "Edmunds is a Express based framework for building web applications with a firm structure to start from.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "tsc --watch",
    "tsc": "tsc",
    "tsc:clean": "rm -rf dist && tsc",
    "tsc:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --exclude '**/*.d.ts' --exclude 'node_modules/**/*.ts' '**/*.ts'",
    "lint:watch": "nodemon --ext ts --exec npm run lint",
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --recursive --exit '**/*.spec.ts'",
    "test:watch": "nodemon --ext ts --exec npm test",
    "docs": "docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LowieHuyghe/edmunds-js.git"
  },
  "keywords": [
    "express",
    "framework",
    "javascript"
  ],
  "author": "Lowie Huyghe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LowieHuyghe/edmunds-js/issues"
  },
  "homepage": "https://github.com/LowieHuyghe/edmunds-js#README",
  "dependencies": {
    "config": "^1.29.2",
    "express": "^4.16.2",
    "joi": "^13.1.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.3",
    "@types/config": "0.0.33",
    "@types/express": "^4.11.0",
    "@types/joi": "^13.0.5",
    "@types/mocha": "^2.2.47",
    "@types/node": "^9.3.0",
    "@types/winston": "^2.3.7",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "codecov": "^3.0.0",
    "docsify-cli": "^4.2.0",
    "es6-promise": "^4.2.4",
    "import-fresh": "^2.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2"
  }
}

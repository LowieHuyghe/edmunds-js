{
  "name": "edmunds",
  "version": "0.0.1",
  "description": "Edmunds is a Express based framework for building web applications with a firm structure to start from.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "tsc --watch",
    "tsc:watch": "tsc --watch",
    "tsc": "tsc",
    "tsc:clean": "rm -rf dist && tsc",
    "lint": "tslint -c tslint.json --exclude '**/*.d.ts' --exclude 'node_modules/**/*.ts' '**/*.ts'",
    "test": "nyc mocha **/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LowieHuyghe/edmunds-js.git"
  },
  "keywords": [
    "express",
    "framework",
    "javascript"
  ],
  "author": "Lowie Huyghe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LowieHuyghe/edmunds-js/issues"
  },
  "homepage": "https://github.com/LowieHuyghe/edmunds-js#README",
  "dependencies": {
    "config": "^1.29.1",
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.0",
    "@types/config": "0.0.33",
    "@types/express": "^4.11.0",
    "@types/mocha": "^2.2.46",
    "@types/node": "^8.5.2",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2"
  }
}

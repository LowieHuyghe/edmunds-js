{
  "name": "edmunds",
  "version": "0.4.0",
  "description": "Edmunds is a Express based framework for building web applications with a firm structure to start from.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "start": "npm run tsc:watch",
    "tsc": "rm -rf dist && tsc",
    "tsc:watch": "tsc --watch",
    "lint": "tslint -c tslint.json --exclude '**/*.d.ts' --exclude 'node_modules/**/*.ts' '**/*.ts'",
    "lint:watch": "nodemon --ext ts --exec npm run lint",
    "test": "NODE_ENV=testing mocha --opts mocha.opts $NODE_DEBUG_OPTION '**/*.spec.ts'",
    "test:cover": "NODE_ENV=testing nyc mocha --opts mocha.opts $NODE_DEBUG_OPTION '**/*.spec.ts'",
    "test:watch": "NODE_ENV=testing mocha --watch --opts mocha.opts $NODE_DEBUG_OPTION '**/*.spec.ts'",
    "docs": "docsify serve docs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/edmundsjs/framework.git"
  },
  "keywords": [
    "express",
    "framework",
    "javascript"
  ],
  "author": "Lowie Huyghe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/edmundsjs/framework/issues"
  },
  "homepage": "https://github.com/edmundsjs/framework#README",
  "peerDependencies": {
    "@google-cloud/storage": "^1.7.0",
    "firebase-admin": "^5.13.1",
    "firebase-functions": "^2.0.1",
    "memcached": "^2.2.2",
    "redis": "^2.8.0",
    "typeorm": "^0.2.7",
    "winston": "^3.0.0",
    "winston-transport": "^4.2.0"
  },
  "dependencies": {
    "commander": "^2.16.0",
    "config": "^1.31.0",
    "express": "^4.16.3",
    "inquirer": "^6.0.0",
    "joi": "^13.4.0",
    "mkdirp": "^0.5.1",
    "reflect-metadata": "^0.1.12"
  },
  "devDependencies": {
    "@google-cloud/storage": "^1.7.0",
    "@types/app-root-path": "^1.2.4",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-http": "^3.0.5",
    "@types/config": "^0.0.34",
    "@types/express": "^4.16.0",
    "@types/inquirer": "^0.0.42",
    "@types/joi": "^13.4.0",
    "@types/memcached": "^2.2.5",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.3",
    "@types/redis": "^2.8.6",
    "@types/sinon": "^5.0.1",
    "@types/winston": "^2.3.9",
    "app-root-path": "^2.1.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "codecov": "^3.0.4",
    "docsify-cli": "^4.2.1",
    "firebase-admin": "^5.13.1",
    "firebase-functions": "^2.0.1",
    "import-fresh": "^2.0.0",
    "memcached": "^2.2.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "redis": "^2.8.0",
    "rmdir": "^1.2.0",
    "sinon": "^6.1.4",
    "source-map-support": "^0.5.6",
    "sql.js": "^0.5.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typeorm": "^0.2.7",
    "typescript": "^2.9.2",
    "winston": "^3.0.0",
    "winston-transport": "^4.2.0"
  }
}
